var mainWin = Ti.UI.createWindow
({	
	backgroundColor: "#fff",
	layout: "horizontal"
}); 

var navWindow = Ti.UI.iOS.createNavigationWindow
({
	window: mainWin
});



var border = Ti.UI.createView
({
	backgroundColor: "#323659",
	height: 1,
	top: 0
});




//////////////////////////////////////////////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////////////////////////



var LoadF = require("GView");

mainWin.add(border);
navWindow.open();











