var mainWin = Ti.UI.createWindow
({	
	backgroundColor: "#fff",
	layout: "horizontal"
}); 


var navWindow = Ti.UI.iOS.createNavigationWindow
({
	window: mainWin
});

var border = Ti.UI.createView
({
	backgroundColor: "#323659",
	left : (Ti.Platform.displayCaps.platformWidth - 100) / 2,
    top : ((Ti.Platform.displayCaps.platformHeight - 50) / 2) + 20,
	width: 100,
	height: 50
	
});




//////////////////////////////////////////////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////////////////////////



var LoadF = require("GView");

mainWin.add(border);
navWindow.open();











