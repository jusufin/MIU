



var MainWindow = Ti.UI.createWindow({
	
	backgroundColor:"#A4A62B"

}); //creates window and sets backround color

var MainView = Ti.UI.createView({
		backgroundColor: "#23456D",
		borderRadius: 5,
		borderWidth: 2,
		width: 300,
		height: 450,
		top: 30
	
});


MainWindow.add(MainView);
MainWindow.open();
